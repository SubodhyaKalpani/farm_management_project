    <div class="col-md-3">
        <div class="col-md-offset-10 col-md-10">
            <input type="button" id="btnSearch" value="Load" class="btn btn-primary btn-user btn-block" onclick="reportLoad();" />
        </div>
    </div>


</div>
<br />


<div class="row"></div>
<div class="row">




</div>


<div class="row" id="divReport">
