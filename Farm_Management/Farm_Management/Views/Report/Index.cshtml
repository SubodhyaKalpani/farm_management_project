<script>
    function reportLoad() {

        var id = 1;
        var searchText = $("#searchText").val();
        var searchText1 = $("#searchText1").val();

        window.open('/Reports/Report.aspx?id='
            + id + '&&searchText=' + searchText + '&&searchText1=' + searchText1, '_blank');

    }



</script>

